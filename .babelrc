{
  "presets": ["next/babel", ["@babel/preset-react", { "runtime": "automatic", "importSource": "@emotion/react" }]],
  "plugins": [
    "@emotion/babel-plugin",
    "inline-react-svg",
    [
      "module-resolver",
      {
        "alias": {
          "root": "./",
          "@Assets": "./assets"
        }
      }
    ]
  ]
}
